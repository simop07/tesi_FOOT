@article{Gilkes2014-rw,
  title    = {Hypoxia and the extracellular matrix: drivers of tumour
              metastasis},
  author   = {Gilkes, Daniele M and Semenza, Gregg L and Wirtz, Denis},
  abstract = {Of the deaths attributed to cancer, 90\% are due to metastasis,
              and treatments that prevent or cure metastasis remain elusive.
              Emerging data indicate that hypoxia and the extracellular matrix
              (ECM) might have crucial roles in metastasis. During tumour
